{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "anyOf": [
    {"required": ["password"]},
    {"required": ["imageURL"]}
  ],
  "properties": {
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "imageURL": {
      "type": "string",
      "minLength": 1
    }
  }
}
