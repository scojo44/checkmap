{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name",
    "regionType"
  ],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "description": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "regionType": {
      "enum": ["State", "County"]
    }
  }
}
